        if (ttt.includes(moment().diff(time, "seconds")))
          return msg.edit(
            `**ğŸ‰ GIVEAWAY ğŸ‰**`,
            embed
              .setColor("#ffb800")
              .setDescription(
                `**Last chance to enter!!!**\nReact with ğŸ‰\nTime remaining: ${cd(
                  new Date().getTime(),
                  time
                )}`
              )
          );
        msg.edit(
          `**ğŸ‰ GIVEAWAY ğŸ‰**`,
          embed.setDescription(
            `React with ğŸ‰ to enter!\nTime remaining: ${cd(
              new Date().getTime(),
              time
            )}`
          )
        );
