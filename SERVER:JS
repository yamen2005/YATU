      stat = 0x00aa4c;
    } else if (user.presence.status === "dnd") {
      //LuXy-SaAD
      stat = 0x9c0000;
    } else if (user.presence.status === "idle") {
      //LuXy-SaAD
      stat = 0xf7c035;
