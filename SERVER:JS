    if (user.presence.status === "online") {
      //LuXy-Saad
      status = ":green_heart:";
    } else if (user.presence.status === "dnd") {
      //LuXy-SaAd
      status = ":heart:";
    } else if (user.presence.status === "idle") {
      //LuXy-Saad
      status = ":yellow_heart:";
    } else if (user.presence.status === "offline") {
      //LuXy-Saad
      status = ":black_heart:";
