    if (i == -1) {
      j = 1;
    }
    if (i == setGame.length - 1) {
      j = -1;
    }
    i = i + j;
    client.user.setGame(setGame[i], `http://www.twitch.tv/idk`);
  }, ms);
  console.log(` Y A M E N |> Name: ${client.user.username}`);
  
