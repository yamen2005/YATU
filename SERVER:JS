    if (user.lastMessage === null) {
      //by aboroh
      messag = "Ù„Ù… ÙŠØ±Ø³Ù„ Ø±Ø³Ø§Ù„Ø©. ";
    } else {
      //by aboroh
      messag = user.lastMessage;
