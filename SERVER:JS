client.on("guildCreate", guild => {
  let channel = client.channels.get(
    guild.channels
      .filter(
        c =>
          c.permissionsFor(client.user).has("SEND_MESSAGES") &&
          c.type === "text"
      )
      .map(r => r.id)[0]
  );

  channel.send("**Ù„Ù…Ø¹Ø±ÙØ© Ø§Ù„Ø§ÙˆØ§Ù…Ø± Ø§Ù„Ø®Ø§ØµÙ‡ Ø¨Ø§Ù„Ø¨ÙˆØª Ø§ÙƒØªØ¨** ``-help``");
}); //LuXy-SaAD

